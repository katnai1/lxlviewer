{
  "name": "vue-client",
  "version": "1.29.5",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "postinstall": "yarn --cwd ../lxljs/ install --frozen-lockfile",
    "lint": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --mode e2e",
    "test:e2e_ci": "vue-cli-service test:e2e --mode e2e --headless"
  },
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@babel/polyfill": "^7.12.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-e2e-cypress": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/test-utils": "^2.3.2",
    "autosize": "^4.0.2",
    "babel-jest": "^29.5.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.7.tar.gz",
    "client-oauth2": "^4.3.3",
    "combokeys": "https://github.com/avocode/combokeys/archive/v3.1.0.tar.gz",
    "core-js": "^3.29.1",
    "core-js-compat": "^3.19.1",
    "focus-trap": "^7.4.0",
    "focus-trap-vue": "^4.0.2",
    "font-awesome": "4.7.0",
    "git-describe": "^4.0.4",
    "json-loader": "^0.5.7",
    "kungbib-styles": "^1.3.3",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "lodash-es": "^4.17.14",
    "lxljs": "file:../lxljs",
    "marked": "^2.0.0",
    "md5": "^2.2.1",
    "modernizr": "^3.6.0",
    "moment": "^2.29.4",
    "path": "^0.12.7",
    "portal-vue": "^1.5.0",
    "querystring-es3": "^0.2.1",
    "style-resources-loader": "^1.3.3",
    "v-tooltip": "^2.0.2-rc.33",
    "vue": "^3.2.44",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue3-click-away": "^1.2.4",
    "vue-clipboard2": "^0.3.0",
    "@vue/compat": "^3.2.44",
    "vue-loader": "^17.0.1",
    "vue-meta": "^2.4.0",
    "vue-resize": "^2.0.0-alpha.1",
    "vue-router": "^4.1.6",
    "vue-simple-spinner": "^1.2.8",
    "vuex": "^4.1.0",
    "webpack": "^5.52.0",
    "webpack-modernizr-loader": "^4.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {},
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "globals": {
      "runtimeConfig": {}
    },
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es|lxljs)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
